<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>FindIn | Sign Up</title>
    <meta name="description" content="RootUI - clean and powerful solution for your Dashboards, Administration areas.">
    <meta name="keywords" content="admin, dashboard, template, react, reactjs, html, jquery, clean">
    <meta name="author" content="nK">
    <link rel="icon" type="image/png" href="./rootui-html/build/assets/images/favicon.png">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,400%7cOpen+Sans:300,400,600%7cPT+Serif:400i">
    <link rel="stylesheet" href="./rootui-html/build/assets/css/bootstrap-custom.css">
    <link rel="stylesheet" href="./rootui-html/build/assets/css/rootui.css">
    <link rel="stylesheet" href="./rootui-html/build/assets/css/rootui-night.css" media="(night)" class="rui-nightmode-link">
    <link rel="stylesheet" href="./rootui-html/build/assets/css/custom.css">
</head>
<body>
    <div class="rui-page-preloader" role="status">
        <div class="rui-page-preloader-inner">
            <div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div>
        </div>
    </div>
    <div class="rui-main">
        <div class="rui-sign">
            <div class="row w-100">
                <div class="col-lg-6">
                    <div class="bg-image">
                        <div style="background-image: url('./rootui-html/build/assets/images/bg-sign.png');"></div>
                    </div>
                </div>
                <div class="col-lg-6 d-flex align-items-center justify-content-center">
                    <form class="form rui-sign-form">
                        <div class="row vertical-gap sm-gap justify-content-center">
                            <div class="col-12">
                                <h1 class="display-4 mb-10 text-center">Sign Up</h1>
                            </div>
                            <div class="col-12">
                                <input class="form-control" placeholder="Name">
                            </div>
                            <div class="col-12">
                                <input type="email" class="form-control" placeholder="Email">
                            </div><div class="col-12">
                                <input type="password" class="form-control" placeholder="Password">
                            </div>
                            <div class="col-12">
                                <div class="custom-control custom-checkbox d-flex justify-content-start">
                                    <input type="checkbox" class="custom-control-input" id="rememberMe">
                                    <label class="custom-control-label text-grey-5 fs-13" for="rememberMe">I have read and agree to the <a href="#" class="text-2">terms and conditions.</a></label>
                                </div>
                            </div>
                            <div class="col-12"><a href="dashboard.html" class="btn btn-brand btn-block text-center">Sign up</a></div>
                            <div class="col-12">
                                <div class="rui-sign-or mt-2 mb-5">or</div>
                            </div><div class="col-12">
                                <ul class="rui-social-links">
                                    <li><a href="dashboard.html" class="rui-social-github"><span class="fab fa-github"></span> Github</a></li>
                                    <li><a href="dashboard.html" class="rui-social-facebook"><span class="fab fa-facebook-f"></span> Facebook</a></li>
                                    <li><a href="dashboard.html" class="rui-social-google"><span class="fab fa-google"></span> Google</a></li>
                                </ul>
                            </div><div class="col-12 text-center text-grey-5 fs-13">Already have an account? <a asp-page="Login" class="text-2">Sign In</a></div>
                        </div>
                    </form>
                </div>
                
            </div>
        </div>
    </div>
    <script src="./rootui-html/build/assets/vendor/jquery/dist/jquery.min.js"></script>
    <script src="./rootui-html/build/assets/vendor/popper.js/dist/umd/popper.min.js"></script>
    <script src="./rootui-html/build/assets/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="./rootui-html/build/assets/vendor/feather-icons/dist/feather.min.js"></script>
    <script src="./rootui-html/build/assets/vendor/fontawesome-free/js/all.js"></script>
    <script src="./rootui-html/build/assets/vendor/fontawesome-free/js/v4-shims.js"></script>
    <script src="./rootui-html/build/assets/js/rootui.js"></script>
    <script src="./rootui-html/build/assets/js/rootui-init.js"></script>
</body>
</html>
